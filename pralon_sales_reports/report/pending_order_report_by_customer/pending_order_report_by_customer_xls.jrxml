	<queryString>
		<![CDATA[-- We are interested only in sale order line (SOL) with a stock move
-- that is pending.  There is a rule concerning SOL product (S),
-- pending stock move product (P), and delivered stock move product (D).

-- S = P | S = D | P = D
-- T     | T     | T (this is normal case, one line): Case A
--                   P1 P1 P1 (Pattern 1)
-- T     | T     | F (impossible)
-- T     | F     | T (impossible)
-- T     | F     | F (this will display two lines): Case B
--                   P1 P1    (Pattern 2)
--                         P2 (Pattern 3)
-- F     | T     | T (impossible)
-- F     | T     | F (this will display two lines): Case C
--                   P1    P1 (Pattern 4)
--                      P2    (Pattern 5)
-- F     | F     | T (this will display two lines): Case D
--                   P1       (Pattern 6)
--                      P2 P2 (Pattern 7)
-- F     | F     | F (this will display three lines): Case E
--                   P1       (Pattern 6)
--                      P2    (Pattern 5)
--                         P3 (Pattern 3)

-- There are seven patterns in total.

-- Because there is no point of showing a SOL or a delivery without
-- any left over, we are only interested in Pattern 1, Pattern 2,
-- Pattern 5, Pattern 7.

SELECT
 *
FROM
(
 (-- Case A with select clause producing Pattern 1
  SELECT
   rp.name AS cust_name,
   so.name AS so_no,
   p.name_template AS prod,
   so.date_confirm AS so_date,
   pending_sm.sched_date AS expected_date,
   sol.product_uom_qty AS sol_qty,
   pu.name AS sol_uom,
   sol.product_uom_qty * pt.weight_net AS sol_weight,
   pending_sm.qty AS do_pend_qty,
   pending_sm_uom.name AS do_pend_uom,
   pending_sm.qty * pending_sm_pt.weight_net AS do_pend_weight,
   delivered_sm.qty AS do_done_qty,
   delivered_sm_uom.name AS do_done_uom,
   delivered_sm.qty * delivered_sm_pt.weight_net AS do_done_weight
  FROM sale_order so
   INNER JOIN res_partner rp
    ON so.partner_id = rp.id
   INNER JOIN sale_order_line sol
    ON so.id = sol.order_id
   INNER JOIN product_product p
    ON sol.product_id = p.id
   INNER JOIN product_template pt
    ON p.product_tmpl_id = pt.id
   INNER JOIN product_uom pu
    ON sol.product_uom = pu.id
   INNER JOIN (
     SELECT
      sm.sale_line_id AS sol_id,
      sm.product_id AS prod_id,
      sm.product_uom AS uom_id,
      SUM(sm.product_qty) AS qty,
      MAX(DATE_TRUNC('day', date_expected)) AS sched_date
     FROM stock_move sm
     WHERE sm.state IN ('confirmed', 'assigned')
     GROUP BY
      sm.sale_line_id,
      sm.product_id,
      sm.product_uom
    ) pending_sm
    ON sol.id = pending_sm.sol_id
   INNER JOIN product_uom pending_sm_uom
    ON pending_sm_uom.id = pending_sm.uom_id
   INNER JOIN product_product pending_sm_p
    ON pending_sm_p.id = pending_sm.prod_id
   INNER JOIN product_template pending_sm_pt
    ON pending_sm_pt.id = pending_sm_p.product_tmpl_id
   LEFT JOIN (
     SELECT
      sm.sale_line_id AS sol_id,
      sm.product_id AS prod_id,
      sm.product_uom AS uom_id,
      SUM(sm.product_qty) AS qty
     FROM stock_move sm
     WHERE sm.state = 'done'
     GROUP BY
      sm.sale_line_id,
      sm.product_id,
      sm.product_uom
    ) delivered_sm
    ON sol.id = delivered_sm.sol_id
   LEFT JOIN product_uom delivered_sm_uom
    ON delivered_sm_uom.id = delivered_sm.uom_id
   LEFT JOIN product_product delivered_sm_p
    ON delivered_sm_p.id = delivered_sm.prod_id
   LEFT JOIN product_template delivered_sm_pt
    ON delivered_sm_pt.id = delivered_sm_p.product_tmpl_id
  WHERE
   so.state NOT IN ('draft', 'cancel', 'done')
   AND (sol.product_id = pending_sm.prod_id
	AND sol.product_id = delivered_sm.prod_id
	AND pending_sm.prod_id = delivered_sm.prod_id) -- Case A
   AND so.date_confirm
    BETWEEN '$P!{FROM_DATE_2_YR}-$P!{FROM_DATE_2_MO}-$P!{FROM_DATE_2_DY}'
     AND '$P!{TO_DATE_2_YR}-$P!{TO_DATE_2_MO}-$P!{TO_DATE_2_DY}'
   AND rp.id IN ($P!{CUSTOMER_IDS})
   AND so.company_id IN ($P!{COMPANY_IDS})
 )
 UNION ALL
 (-- Case B with select clause producing Pattern 2
  SELECT
   rp.name AS cust_name,
   so.name AS so_no,
   p.name_template AS prod,
   so.date_confirm AS so_date,
   pending_sm.sched_date AS expected_date,
   sol.product_uom_qty AS sol_qty,
   pu.name AS sol_uom,
   sol.product_uom_qty * pt.weight_net AS sol_weight,
   pending_sm.qty AS do_pend_qty,
   pending_sm_uom.name AS do_pend_uom,
   pending_sm.qty * pending_sm_pt.weight_net AS do_pend_weight,
   NULL AS do_done_qty,
   NULL AS do_done_uom,
   NULL AS do_done_weight
  FROM sale_order so
   INNER JOIN res_partner rp
    ON so.partner_id = rp.id
   INNER JOIN sale_order_line sol
    ON so.id = sol.order_id
   INNER JOIN product_product p
    ON sol.product_id = p.id
   INNER JOIN product_template pt
    ON p.product_tmpl_id = pt.id
   INNER JOIN product_uom pu
    ON sol.product_uom = pu.id
   INNER JOIN (
     SELECT
      sm.sale_line_id AS sol_id,
      sm.product_id AS prod_id,
      sm.product_uom AS uom_id,
      SUM(sm.product_qty) AS qty,
      MAX(DATE_TRUNC('day', date_expected)) AS sched_date
     FROM stock_move sm
     WHERE sm.state IN ('confirmed', 'assigned')
     GROUP BY
      sm.sale_line_id,
      sm.product_id,
      sm.product_uom
    ) pending_sm
    ON sol.id = pending_sm.sol_id
   INNER JOIN product_uom pending_sm_uom
    ON pending_sm_uom.id = pending_sm.uom_id
   INNER JOIN product_product pending_sm_p
    ON pending_sm_p.id = pending_sm.prod_id
   INNER JOIN product_template pending_sm_pt
    ON pending_sm_pt.id = pending_sm_p.product_tmpl_id
   LEFT JOIN (
     SELECT
      sm.sale_line_id AS sol_id,
      sm.product_id AS prod_id,
      sm.product_uom AS uom_id,
      SUM(sm.product_qty) AS qty
     FROM stock_move sm
     WHERE sm.state = 'done'
     GROUP BY
      sm.sale_line_id,
      sm.product_id,
      sm.product_uom
    ) delivered_sm
    ON sol.id = delivered_sm.sol_id
   LEFT JOIN product_uom delivered_sm_uom
    ON delivered_sm_uom.id = delivered_sm.uom_id
   LEFT JOIN product_product delivered_sm_p
    ON delivered_sm_p.id = delivered_sm.prod_id
   LEFT JOIN product_template delivered_sm_pt
    ON delivered_sm_pt.id = delivered_sm_p.product_tmpl_id
  WHERE
   so.state NOT IN ('draft', 'cancel', 'done')
   AND (sol.product_id = pending_sm.prod_id
	AND (delivered_sm.prod_id IS NULL
	     OR (sol.product_id != delivered_sm.prod_id
		 AND pending_sm.prod_id != delivered_sm.prod_id))) -- Case B
   AND so.date_confirm
    BETWEEN '$P!{FROM_DATE_2_YR}-$P!{FROM_DATE_2_MO}-$P!{FROM_DATE_2_DY}'
     AND '$P!{TO_DATE_2_YR}-$P!{TO_DATE_2_MO}-$P!{TO_DATE_2_DY}'
   AND rp.id IN ($P!{CUSTOMER_IDS})
   AND so.company_id IN ($P!{COMPANY_IDS})
 )
 UNION ALL
 (-- Case C and Case E with select clause producing Pattern 5
  SELECT
   rp.name AS cust_name,
   so.name AS so_no,
   pending_sm_p.name_template AS prod,
   so.date_confirm AS so_date,
   pending_sm.sched_date AS expected_date,
   NULL AS sol_qty,
   NULL AS sol_uom,
   NULL AS sol_weight,
   pending_sm.qty AS do_pend_qty,
   pending_sm_uom.name AS do_pend_uom,
   pending_sm.qty * pending_sm_pt.weight_net AS do_pend_weight,
   NULL AS do_done_qty,
   NULL AS do_done_uom,
   NULL AS do_done_weight
  FROM sale_order so
   INNER JOIN res_partner rp
    ON so.partner_id = rp.id
   INNER JOIN sale_order_line sol
    ON so.id = sol.order_id
   INNER JOIN product_product p
    ON sol.product_id = p.id
   INNER JOIN product_template pt
    ON p.product_tmpl_id = pt.id
   INNER JOIN product_uom pu
    ON sol.product_uom = pu.id
   INNER JOIN (
     SELECT
      sm.sale_line_id AS sol_id,
      sm.product_id AS prod_id,
      sm.product_uom AS uom_id,
      SUM(sm.product_qty) AS qty,
      MAX(DATE_TRUNC('day', date_expected)) AS sched_date
     FROM stock_move sm
     WHERE sm.state IN ('confirmed', 'assigned')
     GROUP BY
      sm.sale_line_id,
      sm.product_id,
      sm.product_uom
    ) pending_sm
    ON sol.id = pending_sm.sol_id
   INNER JOIN product_uom pending_sm_uom
    ON pending_sm_uom.id = pending_sm.uom_id
   INNER JOIN product_product pending_sm_p
    ON pending_sm_p.id = pending_sm.prod_id
   INNER JOIN product_template pending_sm_pt
    ON pending_sm_pt.id = pending_sm_p.product_tmpl_id
   LEFT JOIN (
     SELECT
      sm.sale_line_id AS sol_id,
      sm.product_id AS prod_id,
      sm.product_uom AS uom_id,
      SUM(sm.product_qty) AS qty
     FROM stock_move sm
     WHERE sm.state = 'done'
     GROUP BY
      sm.sale_line_id,
      sm.product_id,
      sm.product_uom
    ) delivered_sm
    ON sol.id = delivered_sm.sol_id
   LEFT JOIN product_uom delivered_sm_uom
    ON delivered_sm_uom.id = delivered_sm.uom_id
   LEFT JOIN product_product delivered_sm_p
    ON delivered_sm_p.id = delivered_sm.prod_id
   LEFT JOIN product_template delivered_sm_pt
    ON delivered_sm_pt.id = delivered_sm_p.product_tmpl_id
  WHERE
   so.state NOT IN ('draft', 'cancel', 'done')
   AND ((sol.product_id != pending_sm.prod_id
	 AND (delivered_sm.prod_id IS NOT NULL
	      AND sol.product_id = delivered_sm.prod_id)
	 AND (delivered_sm.prod_id IS NULL
	      OR pending_sm.prod_id != delivered_sm.prod_id)) -- Case C
	OR (sol.product_id != pending_sm.prod_id
	    AND (delivered_sm.prod_id IS NULL
		 OR (sol.product_id != delivered_sm.prod_id
		     AND pending_sm.prod_id != delivered_sm.prod_id)))) -- Case E
   AND so.date_confirm
    BETWEEN '$P!{FROM_DATE_2_YR}-$P!{FROM_DATE_2_MO}-$P!{FROM_DATE_2_DY}'
     AND '$P!{TO_DATE_2_YR}-$P!{TO_DATE_2_MO}-$P!{TO_DATE_2_DY}'
   AND rp.id IN ($P!{CUSTOMER_IDS})
   AND so.company_id IN ($P!{COMPANY_IDS})
 )
 UNION ALL
 (-- Case D with select clause producing Pattern 7
  SELECT
   rp.name AS cust_name,
   so.name AS so_no,
   pending_sm_p.name_template AS prod,
   so.date_confirm AS so_date,
   pending_sm.sched_date AS expected_date,
   NULL AS sol_qty,
   NULL AS sol_uom,
   NULL AS sol_weight,
   pending_sm.qty AS do_pend_qty,
   pending_sm_uom.name AS do_pend_uom,
   pending_sm.qty * pending_sm_pt.weight_net AS do_pend_weight,
   delivered_sm.qty AS do_done_qty,
   delivered_sm_uom.name AS do_done_uom,
   delivered_sm.qty * delivered_sm_pt.weight_net AS do_done_weight
  FROM sale_order so
   INNER JOIN res_partner rp
    ON so.partner_id = rp.id
   INNER JOIN sale_order_line sol
    ON so.id = sol.order_id
   INNER JOIN product_product p
    ON sol.product_id = p.id
   INNER JOIN product_template pt
    ON p.product_tmpl_id = pt.id
   INNER JOIN product_uom pu
    ON sol.product_uom = pu.id
   INNER JOIN (
     SELECT
      sm.sale_line_id AS sol_id,
      sm.product_id AS prod_id,
      sm.product_uom AS uom_id,
      SUM(sm.product_qty) AS qty,
      MAX(DATE_TRUNC('day', date_expected)) AS sched_date
     FROM stock_move sm
     WHERE sm.state IN ('confirmed', 'assigned')
     GROUP BY
      sm.sale_line_id,
      sm.product_id,
      sm.product_uom
    ) pending_sm
    ON sol.id = pending_sm.sol_id
   INNER JOIN product_uom pending_sm_uom
    ON pending_sm_uom.id = pending_sm.uom_id
   INNER JOIN product_product pending_sm_p
    ON pending_sm_p.id = pending_sm.prod_id
   INNER JOIN product_template pending_sm_pt
    ON pending_sm_pt.id = pending_sm_p.product_tmpl_id
   LEFT JOIN (
     SELECT
      sm.sale_line_id AS sol_id,
      sm.product_id AS prod_id,
      sm.product_uom AS uom_id,
      SUM(sm.product_qty) AS qty
     FROM stock_move sm
     WHERE sm.state = 'done'
     GROUP BY
      sm.sale_line_id,
      sm.product_id,
      sm.product_uom
    ) delivered_sm
    ON sol.id = delivered_sm.sol_id
   LEFT JOIN product_uom delivered_sm_uom
    ON delivered_sm_uom.id = delivered_sm.uom_id
   LEFT JOIN product_product delivered_sm_p
    ON delivered_sm_p.id = delivered_sm.prod_id
   LEFT JOIN product_template delivered_sm_pt
    ON delivered_sm_pt.id = delivered_sm_p.product_tmpl_id
  WHERE
   so.state NOT IN ('draft', 'cancel', 'done')
   AND ((sol.product_id != pending_sm.prod_id
	 AND (delivered_sm.prod_id IS NULL
	      OR sol.product_id != delivered_sm.prod_id)
	 AND (delivered_sm.prod_id IS NOT NULL
	      AND pending_sm.prod_id = delivered_sm.prod_id))) -- Case D
   AND so.date_confirm
    BETWEEN '$P!{FROM_DATE_2_YR}-$P!{FROM_DATE_2_MO}-$P!{FROM_DATE_2_DY}'
     AND '$P!{TO_DATE_2_YR}-$P!{TO_DATE_2_MO}-$P!{TO_DATE_2_DY}'
   AND rp.id IN ($P!{CUSTOMER_IDS})
   AND so.company_id IN ($P!{COMPANY_IDS})
 )
) t
ORDER BY
   cust_name,
   so_no,
   so_date,
   prod
]]>
	</queryString>
	<field name="cust_name" class="java.lang.String"/>
	<field name="so_no" class="java.lang.String"/>
	<field name="so_date" class="java.util.Date"/>
