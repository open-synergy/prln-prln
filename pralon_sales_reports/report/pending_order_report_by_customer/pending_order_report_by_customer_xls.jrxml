	<variable name="distinct_count-global-cust_count" class="java.lang.Integer">
		<variableExpression><![CDATA[$P{VCD}.detectChange("distinct_count-global-cust_count", $F{cust_name})
? $P{distinct_count-global-cust_count}.add($P{distinct_count-global-cust_count}.remove(0) + 1)
: $P{distinct_count-global-cust_count}]]></variableExpression>
	</variable>
	<variable name="sum-cust_name-sol_qty" class="java.math.BigDecimal" resetType="Group" resetGroup="cust_name" calculation="Sum">
		<variableExpression><![CDATA[$F{sol_qty}]]></variableExpression>
	</variable>
	<variable name="sum-cust_name-sol_weight" class="java.math.BigDecimal" resetType="Group" resetGroup="cust_name" calculation="Sum">
		<variableExpression><![CDATA[$F{sol_weight}]]></variableExpression>
	</variable>
	<variable name="sum-cust_name-do_done_qty" class="java.math.BigDecimal" resetType="Group" resetGroup="cust_name" calculation="Sum">
		<variableExpression><![CDATA[$F{do_done_qty}]]></variableExpression>
	</variable>
	<variable name="sum-cust_name-do_done_weight" class="java.math.BigDecimal" resetType="Group" resetGroup="cust_name" calculation="Sum">
