	<queryString>
		<![CDATA[WITH
 invoice_data AS (
  SELECT
   ai.id AS inv_id,
   ail.account_id AS acc_id,
   ((CASE
      WHEN ai.type = 'out_refund'
       THEN -1
      ELSE 1
     END) * ai.amount_total) AS total,
   SUM((CASE
         WHEN ai.type = 'out_refund'
          THEN -1
         ELSE 1
        END) * ail.price_subtotal) AS subtotal,
   SUM((CASE
         WHEN ai.type = 'out_refund'
          THEN -1
         ELSE 1
        END) * ail.quantity * pt.weight_net) AS weight,
   ((CASE
      WHEN ai.type = 'out_refund'
       THEN -1
      ELSE 1
     END) * ai.amount_untaxed) AS untaxed,
   ((CASE
      WHEN ai.type = 'out_refund'
       THEN -1
      ELSE 1
     END) * ai.amount_tax) AS tax
  FROM account_invoice ai
   INNER JOIN account_invoice_line ail
    ON ail.invoice_id = ai.id
   LEFT JOIN product_product p
    ON ail.product_id = p.id
   LEFT JOIN product_template pt
    ON p.product_tmpl_id = pt.id
  WHERE
   ai.date_invoice
    BETWEEN '$P!{FROM_DATE_2_YR}-$P!{FROM_DATE_2_MO}-$P!{FROM_DATE_2_DY}'
     AND '$P!{TO_DATE_2_YR}-$P!{TO_DATE_2_MO}-$P!{TO_DATE_2_DY}'
   AND ai.type IN ('out_refund', 'out_invoice')
   AND ai.state IN ('open', 'paid')
   AND ai.company_id IN ($P!{COMPANY_IDS})
  GROUP BY
   ai.id,
   ail.account_id,
   ai.type,
   ai.amount_total,
   ai.amount_untaxed,
   ai.amount_tax
 ),
 tagged_invoice_data AS (
  SELECT
   data.inv_id,
   col_2_cat.col_idx,
   data.total,
   SUM(data.subtotal) AS subtotal,
   SUM(data.weight) AS weight,
   data.untaxed,
   data.tax
  FROM (
    SELECT DISTINCT
     col_2_cat.col_idx,
     aa.id AS cat_id
    FROM UNNEST(ARRAY[$P!{COL_CAT_NAMES}]) col_2_cat (col_idx INTEGER, cat_code VARCHAR, cat_name VARCHAR)
     INNER JOIN account_account aa
      ON (aa.code, aa.name) = (col_2_cat.cat_code, col_2_cat.cat_name)
   ) col_2_cat
   INNER JOIN invoice_data data
    ON data.acc_id = col_2_cat.cat_id
  GROUP BY
   data.inv_id,
   col_2_cat.col_idx,
   data.total,
   data.untaxed,
   data.tax
 ),
 sales_journal_records AS (
  SELECT
   COALESCE(col_1.inv_id,
            col_2.inv_id,
            col_3.inv_id,
            col_4.inv_id,
            col_5.inv_id,
            col_6.inv_id,
            col_7.inv_id) AS inv_id,
   COALESCE(col_1.total,
            col_2.total,
            col_3.total,
            col_4.total,
            col_5.total,
            col_6.total,
            col_7.total,
            0.0) AS total_amt,
   col_1.subtotal AS subtotal_be,
   col_1.weight AS weight_be,
   col_2.subtotal AS subtotal_ts,
   col_2.weight AS weight_ts,
   col_3.subtotal AS subtotal_pe,
   col_3.weight AS weight_pe,
   col_4.subtotal AS subtotal_hand_made,
   col_5.subtotal AS subtotal_res_goods,
   col_6.subtotal AS subtotal_injection,
   col_7.subtotal AS subtotal_solv_cement,
   COALESCE(col_1.untaxed,
            col_2.untaxed,
            col_3.untaxed,
            col_4.untaxed,
            col_5.untaxed,
            col_6.untaxed,
            col_7.untaxed,
            0.0) AS total,
   COALESCE(col_1.tax,
            col_2.tax,
            col_3.tax,
            col_4.tax,
            col_5.tax,
            col_6.tax,
            col_7.tax,
            0.0) AS tax
  FROM
   (SELECT *
    FROM tagged_invoice_data data
    WHERE col_idx = 1) col_1
   FULL JOIN
   (SELECT *
    FROM tagged_invoice_data data
    WHERE col_idx = 2) col_2
     ON col_1.inv_id = col_2.inv_id
   FULL JOIN
   (SELECT *
    FROM tagged_invoice_data data
    WHERE col_idx = 3) col_3
     ON col_1.inv_id = col_3.inv_id
   FULL JOIN
   (SELECT *
    FROM tagged_invoice_data data
    WHERE col_idx = 4) col_4
     ON col_1.inv_id = col_4.inv_id
   FULL JOIN
   (SELECT *
    FROM tagged_invoice_data data
    WHERE col_idx = 5) col_5
     ON col_1.inv_id = col_5.inv_id
   FULL JOIN
   (SELECT *
    FROM tagged_invoice_data data
    WHERE col_idx = 6) col_6
     ON col_1.inv_id = col_6.inv_id
   FULL JOIN
   (SELECT *
    FROM tagged_invoice_data data
    WHERE col_idx = 7) col_7
     ON col_1.inv_id = col_7.inv_id
 )
SELECT
 ai.date_invoice AS date,
 ai.number AS invoice_no,
 ARRAY_TO_STRING(ARRAY_AGG(at.taxform_id), ', ') AS tax_invoice,
 rp.name AS comp_name,
 sjr.total_amt,
 sjr.subtotal_be,
 sjr.weight_be,
 sjr.subtotal_ts,
 sjr.weight_ts,
 sjr.subtotal_pe,
 sjr.weight_pe,
 sjr.subtotal_hand_made,
 sjr.subtotal_res_goods,
 sjr.subtotal_injection,
 sjr.subtotal_solv_cement,
 sjr.total,
 sjr.tax
FROM sales_journal_records sjr
 INNER JOIN account_invoice ai
  ON sjr.inv_id = ai.id
 INNER JOIN res_partner rp
  ON ai.partner_id = rp.id
 LEFT JOIN account_invoice_tax ait
  ON ait.invoice_id = ai.id
 LEFT JOIN account_taxform at
  ON ait.taxform_id = at.id
GROUP BY
 ai.date_invoice,
 ai.number,
 rp.name,
 sjr.inv_id,
 sjr.total_amt,
 sjr.subtotal_be,
 sjr.weight_be,
 sjr.subtotal_ts,
 sjr.weight_ts,
 sjr.subtotal_pe,
 sjr.weight_pe,
 sjr.subtotal_hand_made,
 sjr.subtotal_res_goods,
 sjr.subtotal_injection,
 sjr.subtotal_solv_cement,
 sjr.total,
 sjr.tax
ORDER BY
 comp_name,
 date,
 invoice_no,
 tax_invoice]]>
	</queryString>
